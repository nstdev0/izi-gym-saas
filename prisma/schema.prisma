generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model Member {
  id String @id @default(cuid())

  firstName String
  lastName  String

  email String @unique
  phone String

  docNumber String @unique

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
